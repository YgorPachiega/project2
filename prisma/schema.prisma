generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas = ["Users", "Clients"]
}

model Users {
  id      Int    @id @default(autoincrement())
  usuario String @unique
  senha   String
  @@schema("Users")
}

model Clients {
  id        String   @unique
  nome      String
  cpf       String
  empresa   String
  createdAt DateTime @default(now())
  @@schema("Clients")
}